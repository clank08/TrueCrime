# Environment Configuration for True Crime Frontend

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api/trpc

# Authentication (OAuth)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id
EXPO_PUBLIC_APPLE_CLIENT_ID=your-apple-client-id

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_ERROR_REPORTING=false
EXPO_PUBLIC_DEBUG_MODE=true

# Development Settings
EXPO_PUBLIC_LOG_LEVEL=debug
EXPO_PUBLIC_MOCK_DATA=true